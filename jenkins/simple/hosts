#<name_of_docker_host> ansible_host=<@IP_of_docker_host>
#docker_cent ansible_host=192.168.0.157
#cdcip ansible_host=192.168.0.128
cidemo ansible_host=192.168.87.134


[docker_host:vars]
### WARNING: ensure that the user below is used ofn remotes hosts
#ansible_user=<name_of_remote_user>
ansible_user=root
### WARNING: ensure that the following path points to the private key used by your ansible host. Below is an example
#ansible_ssh_private_key_file=<path_to_private_ssh-key_on_workstation>
ansible_ssh_private_key_file=/root/.ssh/id_rsa
